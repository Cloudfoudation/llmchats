{
  "name": "legaiai",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "env-cmd -f .env.development.local next dev",
    "devsg": "env-cmd -f .env.development.sg next dev",
    "dev:test": "env-cmd -f .env.test.local next dev --port 3030",
    "dev:test-backend": "env-cmd -f .env.test.backend next dev --port 3030",
    "build": "next build",
    "buildsg": "env-cmd -f .env.production.sg next build",
    "builddevsg": "env-cmd -f .env.development.sg next build",
    "buildburnersg": "env-cmd -f .env.production.sg.burner next build",
    "buildmgz01sg": "env-cmd -f .env.production.sg.mgz01 next build",
    "build:test": "env-cmd -f .env.test.local next build",
    "build:test-backend": "env-cmd -f .env.test.backend next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest tests/unit",
    "test:integration": "jest tests/integration",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:test-config": "playwright test --config=playwright.config.test.ts",
    "test:backend": "playwright test --config=playwright.config.simple.ts",
    "test:backend:ui": "playwright test --config=playwright.config.simple.ts --ui",
    "test:backend:debug": "playwright test --config=playwright.config.simple.ts --debug",
    "test:backend:headed": "playwright test --config=playwright.config.simple.ts --headed",
    "test:full-suite": "./scripts/run-full-tests.sh",
    "test:smoke": "playwright test tests/e2e/smoke/ --config=playwright.config.simple.ts --reporter=line",
    "test:auth": "playwright test tests/e2e/auth/ --config=playwright.config.simple.ts --reporter=line",
    "test:chat": "playwright test tests/e2e/chat/ --config=playwright.config.simple.ts --reporter=line",
    "test:chat:basic": "playwright test tests/e2e/chat/full-chat-testing.spec.ts --config=playwright.config.simple.ts --reporter=line",
    "test:chat:advanced": "playwright test tests/e2e/chat/advanced-conversations.spec.ts --config=playwright.config.simple.ts --reporter=line",
    "test:api": "playwright test tests/e2e/api/ --config=playwright.config.simple.ts --reporter=line",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:e2e",
    "test:ci": "npm run test:coverage && npm run test:e2e",
    "test:performance": "playwright test --grep=\"@performance\"",
    "test:security": "playwright test --grep=\"@security\"",
    "test:accessibility": "playwright test --grep=\"@a11y\"",
    "test:api": "playwright test --grep=\"@api\"",
    "test:setup": "./scripts/setup-test-env.sh",
    "test:cleanup": "node scripts/cleanup-test-data.js",
    "test:docker": "docker-compose -f docker-compose.test.yml up --build",
    "test:docker:run": "docker-compose -f docker-compose.test.yml run test-runner",
    "deploy:test-backend": "./scripts/deploy-test-backend.sh",
    "cleanup:test-backend": "./scripts/cleanup-test-backend.sh"
  },
  "dependencies": {
    "@aws-amplify/auth": "^6.8.2",
    "@aws-sdk/client-bedrock": "^3.703.0",
    "@aws-sdk/client-bedrock-agent": "^3.714.0",
    "@aws-sdk/client-bedrock-agent-runtime": "^3.714.0",
    "@aws-sdk/client-bedrock-runtime": "^3.699.0",
    "@aws-sdk/client-dynamodb": "^3.817.0",
    "@aws-sdk/client-s3": "3.726.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@tabler/icons-react": "^3.24.0",
    "@tailwindcss/typography": "^0.5.15",
    "aws-amplify": "^6.10.2",
    "axios": "^1.7.9",
    "clsx": "^1.2.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^11.14.4",
    "jwt-decode": "^4.0.0",
    "lucide-react": "^0.244.0",
    "next": "15.1.0",
    "next-intl": "^3.22.0",
    "next-themes": "^0.2.1",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-icons": "^5.4.0",
    "react-markdown": "^9.0.1",
    "react-syntax-highlighter": "^15.6.1",
    "rehype-raw": "^7.0.0",
    "rehype-sanitize": "^6.0.0",
    "remark-gfm": "^4.0.0",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.6",
    "uuid": "^11.0.3",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.53.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.2",
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "dotenv": "^17.1.0",
    "dotenv-cli": "^8.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^9.17.0",
    "eslint-config-next": "15.1.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "msw": "^2.10.3",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.47",
    "tailwindcss": "^3.4.14",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  },
  "overrides": {
    "@types/react": "19.0.1",
    "@types/react-dom": "19.0.2"
  }
}
